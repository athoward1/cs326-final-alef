{
    "name": "cs326-final-alef",
    "type": "module",
    "version": "1.0.0",
    "description": "a website for doing stuff",
    "devDependancies": {
        "eslint": "^7.11.0",
        "http-server": "^0.12.3"
    },
    "eslintConfig": {
        "env": {
            "browser": true,
            "es2021": true,
            "node": true
        },
        "extends": "eslint:recommended",
        "parserOptions": {
            "ecmaVersion": 12,
            "sourceType": "module"
        },
        "rules": {
            "consistent-return": "error",
            "no-var": "error",
            "eqeqeq": "error",
            "semi": [
                "error",
                "always"
            ],
            "curly": [
                "warn",
                "all"
            ],
            "prefer-const": [
                "warn",
                {}
            ]
        }
    },
    "scripts": {
        "start": "node server/index.js"
    },
    "dependencies": {
        "bcrypt": "^5.0.0",
        "bcryptjs": "^2.4.3",
        "eslint": "^7.14.0",
        "express": "^4.17.1",
        "pg-promise": "^10.7.3"
    },
    "engines": {
        "node": "14.x"
    }
}
